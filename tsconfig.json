{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "lib": [
            "es2017",
            "esnext.asynciterable",
            "dom"
        ],
        "declaration": true,
        "sourceMap": true,
        "inlineSources": true,
        "outDir": "./dist/",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": []
}
